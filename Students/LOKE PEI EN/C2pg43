#include<iostream>
using namespace std;
int main()
{
    int n; double m; char x[1], z;

    cout << "Enter an integer:";
    cin >> n;


    cout << "Enter a decimal number:";
    cin >> m;

    cout << "Enter a single character:";
    cin >> x;

    cout << "Enter an array of characters:";
    cin >> z;

    return 0;

}
